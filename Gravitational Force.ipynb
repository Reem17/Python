   "source": [
    "#Gravitational Force\n",
    "G= float(input(\"Enter Gravitational constant: \"))\n",
    "M= float(input(\"Enter mass of Earth: \"))\n",
    "m= float(input(\"Enter mass of Moon: \"))\n",
    "R= float(input(\"Enter distance between them: \"))\n",
    "F= G * M * m / pow(R,2)\n",
    "print(\"Gravitational force = {}\".format(F))"
   ]
  }
